<?xml version="1.0" encoding="UTF-8"?>
<schema
  xmlns="http://www.concrete5.org/doctrine-xml/0.5"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd">
    <table name="abcFormAutoReplies">
        <field name="farID" type="integer" size="10">
          <unsigned/>
          <autoincrement/>
          <key/>
        </field>
        <field name="qID" type="integer" size="10"></field>
        <field name="qSubject" type="text"></field>
        <field name="qHtml" type="text"></field>
        <field name="fromName" type="text" size="255"></field>
        <field name="fromMail" type="text" size="255"></field>
    </table>
</schema>
